<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <RootNamespace>WPF_FakeGoogle</RootNamespace>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="img\board.png" />
    <None Remove="img\camera.png" />
    <None Remove="img\mike.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="img\board.png" />
    <Resource Include="img\camera.png" />
    <Resource Include="img\mike.png" />
  </ItemGroup>

</Project>
